{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "space-before-function-paren": "off",
    "semi": "error",
    "no-unused-vars": "off",
    "arrow-body-style": "off",
    "no-console": "off",
    "arrow-parens": "off",
    "object-shorthand": "off"
  }
}